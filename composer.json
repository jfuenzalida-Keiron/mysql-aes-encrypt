{
  "name": "jfelixstudio/aesencrypt",
  "type": "library",
  "description": "Laravel MySql AES Encrypt/Decrypt",
  "keywords": [
    "laravel",
    "mysql",
    "aes",
    "encrypt"
  ],
  "homepage": "https://github.com/jfelixstudio/mysql-aes-encrypt",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Jhon Fuenzalida",
      "email": "jhon_fuenzalida@hotmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/config": "^8.0",
    "illuminate/database": "^8.0",
    "illuminate/support": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "jfelix\\AESEncrypt\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "JfelixStudio\\AESEncrypt\\AesEncryptServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
